getgenv().autojoin = false
getgenv().autostart = false
getgenv().restart = false
function doJoin()
spawn(function()
    while getgenv().autojoin == true do
        local A_1 = "CreateRoom"
    local A_2 = 
    {
        ["Difficulty"] = "Easy", 
        ["FriendsOnly"] = true, 
        ["MapName"] = "Titan Dimension", 
        ["Hardcore"] = true,
    }
    local Event = game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction
    Event:InvokeServer(A_1, A_2)
    wait()
    end
end)
end
spawn(function()
while getgenv().autojoin == true do
    local A_1 = "TeleportPlayers"
local Event = game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction
Event:InvokeServer(A_1)
wait()
end
end)
function autoRestart()
while getgenv().restart == true do
  local A_1 = "RetryDungeon"
local Event = game:GetService("ReplicatedStorage").RemoteEvents.MainRemoteEvent
Event:FireServer(A_1)
end 
end
local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("CHAD") -- Creates the window

local b = w:CreateFolder("Auto Restart") -- Creates the folder(U will put here your buttons,etc)

local c = w:CreateFolder("Game Settings") -- Creates the folder(U will put here your buttons,etc)

b:Toggle("Game Settings",function(bool)
    getgenv().autojoin = bool
    print('autojoin is: ',bool)
    if bool then
        doJoin()           
      end
end)
b:Toggle("Auto Restart",function(bool)
    getgenv().autojoin = bool
    print('autojoin is: ',bool)
    if bool then
        autoRestart()        
      end
end)
